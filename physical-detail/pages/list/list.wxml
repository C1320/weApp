<!--体检记录列表界面-->
<view class="suifanList" wx:if="{list.length>0}">
  <view wx:for="{{list}}" wx:key="folup_no" class="item card" >
      <view class="doc_name">责任医师：{{item.doc_name}}</view>
      <view class="time">体检时间：{{item.phy_exam_date}}</view>
      <view class="org_name pin bounce iconfont icon-position">{{item.org_name}}</view>
      <view class="more iconfont icon-youjiantou" catchtap="handitem" data-folup_no="{{item.phy_exam_no}}">查看详情</view>
  </view>
</view>

<view class="empty" wx:else>
  <van-empty description="暂无数据" />
</view>